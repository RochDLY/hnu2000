  school = {M\'edecine du Travail Interentreprises de l'Is\`ere [MT2i]},
  file = {/home/user/Zotero/storage/IJMQHZ4H/Ronda - 2013 - Logiciels libres et ERPs Open Source : maturité de.pdf}
